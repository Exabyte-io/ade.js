{
    "all": true,
    "include": [
        "src/**/*.js",
        "src/**/*.jsx",
        "src/**/*.ts"
    ],
    "exclude": [
        "tests/**/*",
        "**/*.test.ts",
        "**/*.spec.ts",
        "**/*.d.ts"
    ],
    "reporter": [
        "text",
        "html",
        "lcov"
    ],
    "check-coverage": true,
    "branches": 80,
    "lines": 85,
    "functions": 80,
    "statements": 85,
    "sourceMap": true,
    "instrument": true,
    "require": [
        "ts-node/register"
    ]
}

